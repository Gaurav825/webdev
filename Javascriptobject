const car = {type:"Fiat", model:"500", color:"white"};
console.log(car);
const p = {
  firstName: "John",
  lastName: "Doe",
  age: 50,
  eyeColor: "blue"
};
// access the property
console.log(p);
console.log(p.firstName);
console.log(car["color"]);

const person = {
  firstName: "John",
  lastName : "Doe",
  id       : 5566,
  fullName : function() {
    return this.firstName + " " + this.lastName;
  }
};
console.log(person.fullName())
